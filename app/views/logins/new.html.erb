<%= form_for(@log) do |f| %>
  <div class="form-group">
    <%= f.label :login%>
    <%= f.text_field :login, required: true, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label(:password )%>
    <%= f.text_field(:password, required: true, class: 'form-control' )%>
  </div>
  <%= f.submit 'Submit', class: 'btn btn-default' %>
<% end %>
