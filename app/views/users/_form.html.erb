<%= form_tag "/users/#{@user["user"]["id"]}", method: :patch do  %>
  <table id="edit-table" class="table table-bordered" style="display: none;">
    <tr>
      <th>Category</th>
      <th>Multiplier</th>
    </tr>
    <tr>
      <td>Bedroom</td>
      <td><%= number_field_tag :bedroom_mult, @user["user"]["bedroom_mult"], class: "form-control", step: :any %></td>
    </tr>
    <tr>
      <td>Seating</td>
      <td><%= number_field_tag :seating_mult, @user["user"]["seating_mult"], class: "form-control", step: :any %></td>
    </tr>
    <tr>
      <td>Dining</td>
      <td><%= number_field_tag :dining_mult, @user["user"]["dining_mult"], class: "form-control", step: :any %></td>
    </tr>
  </table>
  <% hidden_field_tag :id, @user["user"]["id"] %>
  <%= submit_tag "Submit", class: "btn btn-default", id:"edit-submit" %>
<% end %>
